PROGRAM
-------
#include <string>
#include <iostream>
using namespace std;

int main() {
    string str;
    int len;
    cin>>str;
    len=str.size();
    for(int i = 0; i < len; i++) {
        if(str[i]==',')
        cout<<endl;
        else
        cout<<str[i];
    }
    
    return 0;
}
