#include <iostream>
using namespace std;
void max(){
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    if(a>b&&a>c&&a>d)
    cout<<a;
    else if(b>c&&b>d&&b>a)
    cout<<b;
    else if(c>a&&c>b&&c>d)
    cout<<c;
    else
    cout<<d;
}
int main() {
    max();
    return 0;
}
